{
    "buildCommand": "npm run build",
    "installCommand": "npm install",
    "devCommand": "cd frontend && npm run dev",
    "outputDirectory": "frontend/dist",
    "framework": null,
    "functions": {
        "api/index.js": {
            "includeFiles": "backend/dist/**"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api"
        }
    ]
}